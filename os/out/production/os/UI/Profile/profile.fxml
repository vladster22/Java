<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefWidth="275.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Profile.Controller">
   <children>
      <HBox>
         <children>
            <Label prefWidth="70.0" text="Username" HBox.hgrow="SOMETIMES">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="username" disable="true" editable="false" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label prefWidth="70.0" text="Password" HBox.hgrow="SOMETIMES">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </Label>
            <PasswordField fx:id="password" disable="true" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </PasswordField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label prefWidth="70.0" text="Options" HBox.hgrow="SOMETIMES">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </Label>
            <CheckBox fx:id="admin" disable="true" mnemonicParsing="false" text="Admin">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="main" disable="true" mnemonicParsing="false" text="Main">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </CheckBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT">
         <items>
            <Button mnemonicParsing="false" onAction="#close" prefWidth="75.0" text="Close">
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </Button>
            <Button fx:id="save" disable="true" mnemonicParsing="false" onAction="#save" prefWidth="75.0" text="Save">
               <font>
                  <Font name="Consolas" size="12.0" />
               </font>
            </Button>
         </items>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ToolBar>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</VBox>
